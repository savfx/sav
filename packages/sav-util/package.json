{
  "name": "sav-util",
  "version": "2.0.6",
  "description": "util library for savjs",
  "main": "dist/sav-util.cjs.js",
  "module": "dist/sav-util.es.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "start": "watch -p \"+(test|src|lib)/*.js\" -c \"npm run lint -- --fix && npm test\"",
    "build": "rollup -c",
    "lint": "standard rollup.config.js test/** src/** lib/**",
    "fix": "npm run lint -- --fix",
    "test": "nyc ava ./test",
    "t": "ava ./test/tmpl.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savfx/savjs.git"
  },
  "homepage": "https://github.com/savfx/savjs#readme",
  "author": {
    "name": "jetiny",
    "email": "86287344@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/savfx/savjs/issues"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "test"
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
